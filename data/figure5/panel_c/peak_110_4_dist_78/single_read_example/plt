set yra [0:2.2]
set xra [-175:150]
unset key
set ytics ("Unmethylated C" 1, "Methylated C" 2)
set size ratio 0.2
unset x2tics
unset y2tics
set xtics scale 0
set ytics scale 0
set terminal pdfcairo
